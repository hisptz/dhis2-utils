{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/packages/ui/build",
        "{projectRoot}/packages/ui",
        "{projectRoot}/packages/lib",
        "{projectRoot}/packages/analytics"
      ]
    },
    "start": {
      "dependsOn": [
        "^start",
        "^build"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    }
  }
}
