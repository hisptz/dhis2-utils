{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/packages/analytics/build",
        "{projectRoot}/packages/docs/build",
        "{projectRoot}/packages/lib/build",
        "{projectRoot}/packages/storybook/storybook-static",
        "{projectRoot}/packages/ui/build"
      ]
    }
  }
}
