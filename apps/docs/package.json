{
  "name": "@hisptz/docs",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently -n \"proxy|sb\" --name-separator \"|\" 'bun run server.ts' 'storybook dev -p 6006'",
    "build": "storybook build --docs",
    "preview-storybook": "serve storybook-static",
    "test": "concurrently --success 'command-1' -k -n \"dev|test\" --name-separator \"|\" 'storybook dev -p 6006' 'wait-on tcp:6006 && test-storybook' ",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "lint": "eslint ./stories/*.stories.tsx --max-warnings 0"
  },
  "dependencies": {
    "@dhis2/app-runtime": "^3.14.0",
    "@dhis2/ui": "^10.3.1",
    "@hisptz/dhis2-analytics": "workspace:*",
    "@hisptz/dhis2-scorecard": "workspace:*",
    "@hisptz/dhis2-ui": "workspace:*",
    "@hisptz/dhis2-utils": "workspace:*",
    "@storybook/addon-interactions": "^8.6.12",
    "dotenv": "^16.4.7",
    "http-proxy": "^1.18.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-actions": "^8.6.12",
    "@storybook/addon-coverage": "^1.0.5",
    "@storybook/addon-docs": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/builder-webpack5": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/react-webpack5": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/test-runner": "^0.22.0",
    "@types/http-proxy": "^1.17.16",
    "@types/node": "^20.10.8",
    "@types/react": "^18.3.18",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-storybook": "^0.12.0",
    "playwright": "^1.40.1",
    "serve": "^14.2.1",
    "storybook": "^8.6.12",
    "tsup": "8.3.5",
    "typescript": "^5.3.3",
    "vite": "6.2.6",
    "wait-on": "^7.2.0"
  }
}
