{
  "name": "@hisptz/dhis2-ui",
  "version": "1.0.1",
  "description": "A collection of reusable complex DHIS2 react ui components.",
  "license": "BSD-3-Clause",
  "scripts": {
    "build:ui": "d2-app-scripts build --no-verify",
    "build:types": "tsc --project tsconfig.build.json",
    "postbuild": "yarn build:types",
    "start": "d2-app-scripts start",
    "test": "cypress run --component",
    "test:open": "cypress open --component",
    "publish:ui": "yarn publish --access public"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.1.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "@cypress/react": "^7.0.2",
    "@cypress/vite-dev-server": "^5.0.2",
    "@dhis2/cli-app-scripts": "^10.2.0",
    "@dhis2/cypress-commands": "^9.0.2",
    "@dhis2/cypress-plugins": "^9.0.2",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/react-color": "^3.0.6",
    "@types/styled-components": "^5.1.26",
    "cypress": "^12.3.0",
    "lodash": "^4.17.21",
    "react": "16.14.0",
    "react-hook-form": "^7.42.1",
    "typescript": "^4.9.4",
    "vite": "3"
  },
  "dependencies": {
    "@dhis2/app-runtime": "^3.7.0",
    "@hisptz/dhis2-utils": "^1.0.1",
    "classnames": "^2.3.2",
    "react-color": "^2.19.3",
    "styled-components": "^5.3.6",
    "usehooks-ts": "^2.9.1"
  },
  "main": "./build/cjs/index.js",
  "module": "./build/es/index.js",
  "types": "./build/types/index.d.ts",
  "exports": {
    "import": "./build/es/index.js",
    "require": "./build/cjs/index.js"
  },
  "keywords": [
    "react",
    "dhis2",
    "dhis2-ui",
    "hisptz"
  ],
  "author": "HISP Tanzania",
  "files": [
    "build/**",
    "postinstall.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hisptz/dhis2-utils.git"
  },
  "bugs": {
    "url": "https://github.com/hisptz/dhis2-utils/issues"
  },
  "peerDependencies": {
    "lodash": "^4",
    "react-hook-form": "^7"
  }
}
