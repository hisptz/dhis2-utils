{
  "name": "@hisptz/dhis2-analytics",
  "version": "1.0.46",
  "description": "A collection of reusable react components for visualizing analytics data from DHIS2",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "d2-app-scripts build --no-verify --mode production",
    "build:types": "tsc --project tsconfig.build.json",
    "postbuild": "yarn build:types",
    "start": "d2-app-scripts start",
    "test": "d2-app-scripts test --passWithNoTests",
    "deploy": "d2-app-scripts deploy",
    "publish:analytics": "yarn publish --access public"
  },
  "devDependencies": {
    "@cypress/react": "^7.0.2",
    "@dhis2/cli-app-scripts": "^10.2.0",
    "@dhis2/cypress-commands": "^9.0.2",
    "@dhis2/cypress-plugins": "^9.0.2",
    "@dhis2/ui": "^8.13.0",
    "@google/earthengine": "^0.1.336",
    "@types/d3-color": "^3.1.0",
    "@types/d3-scale": "^4.0.3",
    "@types/leaflet": "^1.9.0",
    "@types/lodash": "^4.14.191",
    "@types/react-helmet": "^6.1.6",
    "cypress": "^12.3.0",
    "lodash": "^4.17.21",
    "react-hook-form": "^7.43.9",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@dhis2/app-runtime": "^3.7.0",
    "@hisptz/dhis2-ui": "^1.0.45",
    "@hisptz/dhis2-utils": "*",
    "@react-leaflet/core": "^2.1.0",
    "async-es": "^3.2.4",
    "d3-color": "^3.1.0",
    "d3-scale": "^4.0.2",
    "highcharts": "^10.3.2",
    "highcharts-react-official": "^3.1.0",
    "leaflet": "^1.9.3",
    "leaflet-easyprint": "^2.1.9",
    "leaflet.fullscreen": "^2.4.0",
    "react-circular-progressbar": "^2.1.0",
    "react-error-boundary": "^4.0.4",
    "react-helmet": "^6.1.0",
    "react-leaflet": "^4.2.0",
    "react-leaflet-custom-control": "^1.3.1",
    "react-spring": "^9.6.1",
    "screenfull": "^6.0.2",
    "styled-jsx": "^5.1.2",
    "usehooks-ts": "^2.9.1"
  },
  "main": "./build/cjs/index.js",
  "module": "./build/es/index.js",
  "types": "./build/types/index.d.ts",
  "exports": {
    "import": "./build/es/index.js",
    "require": "./build/cjs/index.js"
  },
  "keywords": [
    "react",
    "dhis2",
    "dhis2-analytics",
    "hisptz"
  ],
  "author": "HISP Tanzania",
  "files": [
    "build/**",
    "package.json",
    "!**/*.test.js",
    "!**/*.stories.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hisptz/dhis2-utils.git"
  },
  "bugs": {
    "url": "https://github.com/hisptz/dhis2-utils/issues"
  },
  "peerDependencies": {
    "@dhis2/d2-i18n": "^1.1",
    "@dhis2/ui": "^8.13",
    "lodash": "^4",
    "react": "*",
    "react-hook-form": "^7.43",
    "recoil": "*"
  }
}
